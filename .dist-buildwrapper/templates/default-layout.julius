function processAmount(){
	var x=document.getElementById("receiptfield");
	var y=document.getElementById("receipType");
	if (y.value == 2) {
		x.value=0.00
		x.disabled=true 
	}else
	{
		x.disabled=false
	}
	} 




function submitForm(){
	
	var form=document.getElementById("receiptForm");
	var x=document.getElementById("receiptfield");
	x.disabled=false
	form.submit()
	
	
	
	
	
}